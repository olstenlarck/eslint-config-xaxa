{
  "name": "eslint-config-xaxa",
  "description": "Shareable config for `xaxa` - zero-config linting, powered by few amazing unicorns, AirBnB & Prettier.",
  "license": "Apache-2.0",
  "licenseStart": "2018",
  "scripts": {
    "lint": "eslint src test --fix -f codeframe",
    "test": "nyc asia",
    "gitadd": "git status --porcelain && git add -A",
    "commit-dry": "yarn gitadd && gitcommit -sS",
    "precommit": "yarn lint && yarn test && yarn gitadd",
    "commit": "gitcommit -sS"
  },
  "dependencies": {
    "babel-eslint": "^10.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-flowtype": "^4.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-unicorn": "^4.0.3",
    "prettier": "^1.13.7"
  },
  "devDependencies": {
    "ascjs": "^3.0.0",
    "asia": "^0.19.6",
    "gitcommit": "^1.0.8",
    "new-release": "^5.0.0",
    "nyc": "^14.0.0"
  },
  "files": [
    "src"
  ],
  "main": "src/index.js",
  "version": "0.0.0-semantically-released",
  "repository": "olstenlarck/eslint-config-xaxa",
  "homepage": "https://github.com/olstenlarck/eslint-config-xaxa",
  "author": "Charlike Mike Reagent (https://i.am.charlike.online)",
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": "./src/index.js"
  },
  "renovate": {
    "extends": "tunnckocore",
    "packageRules": [
      { 
        "packageNames": ["eslint-plugin-node"],
        "allowedVersions": "< 7"
      },
      { 
        "packageNames": ["eslint"], 
        "allowedVersions": "< 5"
      },
      {
        "packageNames": ["eslint-plugin-unicorn"],
        "allowedVersions": "< 5"
      }
    ]
  },
  "nyc": {
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "lines": 0,
    "cache": true,
    "check-coverage": true,
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": [
      "test"
    ],
    "extension": [
      ".js",
      ".jsx",
      ".mjs"
    ]
  },
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 3,
      "footer": ""
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "asia",
        "gitcommit",
        "new-release",
        "xaxa",
        "charlike"
      ]
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "new-release",
      "semantic-release",
      "xaxa"
    ]
  }
}
